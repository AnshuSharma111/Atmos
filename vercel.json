{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/socketio", "dest": "/api/socketio.js" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/socket.io/(.*)", "dest": "/api/socketio.js" },
    { "src": "/viewer", "dest": "/viewer.html" },
    { "src": "/viewer.html", "dest": "/viewer.html" },
    { "src": "/index", "dest": "/index.html" },
    { "src": "/", "dest": "/viewer.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}